<?xml version="1.0" encoding="UTF-8" ?>
<class name="OpenXRAndroidPassthroughCameraStateExtensionWrapper" inherits="OpenXRExtensionWrapperExtension" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		Wraps the [code]XR_ANDROID_passthrough_camera_state[/code] extension.
	</brief_description>
	<description>
		Wraps the [code]XR_ANDROID_passthrough_camera_state[/code] extension.
	</description>
	<tutorials>
	</tutorials>
	<methods>
		<method name="get_passthrough_camera_state">
			<return type="int" enum="OpenXRAndroidPassthroughCameraStateExtensionWrapper.PassthroughCameraState" />
			<description>
				Returns the current [enum PassthroughCameraState].
			</description>
		</method>
	</methods>
	<constants>
		<constant name="PASSTHROUGH_CAMERA_STATE_DISABLED" value="0" enum="PassthroughCameraState">
			The camera has been disabled by an app, the system or the user.
		</constant>
		<constant name="PASSTHROUGH_CAMERA_STATE_INITIALIZING" value="1" enum="PassthroughCameraState">
			The camera is still coming online and not yet ready to use.
		</constant>
		<constant name="PASSTHROUGH_CAMERA_STATE_READY" value="2" enum="PassthroughCameraState">
			The camera is ready to use.
		</constant>
		<constant name="PASSTHROUGH_CAMERA_STATE_ERROR" value="3" enum="PassthroughCameraState">
			The camera is in an unrecoverable error state.
		</constant>
	</constants>
</class>
